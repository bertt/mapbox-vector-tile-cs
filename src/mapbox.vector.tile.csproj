<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <LangVersion>12</LangVersion>
    <Nullable>enable</Nullable>
    <Version>5.2.3</Version>
	<AssemblyVersion>5.2.3.0</AssemblyVersion>
	<FileVersion>5.2.3.0</FileVersion>
	<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>mapbox-vector-tile</PackageId>
    <Authors>Bert Temme</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Company />
    <Product />
    <PackageReleaseNotes />
    <Description>.NET library for encoding/decoding Mapbox Vector tiles</Description>
    <RepositoryUrl>https://github.com/bertt/mapbox-vector-tile-cs</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE" Pack="true" PackagePath="\" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="Properties\**" />
    <EmbeddedResource Remove="Properties\**" />
    <None Remove="Properties\**" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="mapbox.vector.tile.v2.ncrunchproject" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="protobuf-net" Version="3.2.52" />
  </ItemGroup>

</Project>
